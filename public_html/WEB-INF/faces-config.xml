<?xml version="1.0" encoding="ISO-8859-1"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee">
    <navigation-rule>
        <from-view-id>/SplyzeAdmin.jsf</from-view-id>
        <navigation-case>
            <from-outcome>registerEvent</from-outcome>
            <to-view-id>/RegisterEvent.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registerLocation</from-outcome>
            <to-view-id>/RegisterLocation.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule/>
    <navigation-rule>
        <from-view-id>/RegisterLocation.jsf</from-view-id>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/SplyzeAdmin.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>next</from-outcome>
            <to-view-id>/RegisterLocationAreas.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/RegisterLocationAreas.jsf</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/RegisterLocation.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>next</from-outcome>
            <to-view-id>/RegisterLocationRooms.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/RegisterLocationRooms.jsf</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/RegisterLocationAreas.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>finish</from-outcome>
            <to-view-id>/ConfirmLocationDetails.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ConfirmLocationDetails.jsf</from-view-id>
        <navigation-case>
            <from-outcome>submit</from-outcome>
            <to-view-id>/SplyzeAdmin.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/RegisterLocationRooms.jsf</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>exception</from-outcome>
            <to-view-id>/ShowException.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ShowException.jsf</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/SplyzeAdmin.jsf</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>